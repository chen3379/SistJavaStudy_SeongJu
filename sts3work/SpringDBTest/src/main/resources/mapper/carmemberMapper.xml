<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="db.data.carmember.CarMemberDao" >
 <insert id="insertMember" parameterType="memdto">
 insert into cmember (name,hp,addr,gaipday)values (#{name},#{hp},#{addr},now())
 </insert>
 <delete id="deleteMember" parameterType="int">
 delete from cmember where num=#{num}
 </delete>
 <select id="selectMember" resultType="memdto" parameterType="int">
 select * from cmember where num=#{num}
 </select>
 <update id="updateMember" parameterType="memdto">
 update cmember set name=#{name},hp=#{hp},addr=#{addr} where num=#{num}
 </update>
 <select id="selectAllMember" resultType="memdto">
 select * from cmember order by num desc
 </select>
 </mapper>